{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "1.6.6",
    "@mdx-js/react": "1.6.6",
    "@rebass/forms": "^4.0.6",
    "@rebass/preset": "4.0.5",
    "deep-map": "^2.0.0",
    "gatsby": "2.23.21",
    "gatsby-background-image": "^1.1.1",
    "gatsby-image": "2.4.12",
    "gatsby-plugin-manifest": "2.4.17",
    "gatsby-plugin-mdx": "1.2.22",
    "gatsby-plugin-offline": "3.2.16",
    "gatsby-plugin-pinterest": "^3.0.0",
    "gatsby-plugin-react-helmet": "3.3.9",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-sharp": "2.6.18",
    "gatsby-remark-copy-linked-files": "2.3.10",
    "gatsby-remark-images": "3.3.17",
    "gatsby-remark-reading-time": "1.1.0",
    "gatsby-remark-relative-source": "0.0.4",
    "gatsby-source-filesystem": "2.3.18",
    "gatsby-source-instagram": "0.8.0",
    "gatsby-source-wordpress": "3.3.19",
    "gatsby-transformer-json": "^2.4.10",
    "gatsby-transformer-sharp": "2.5.10",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.15",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-icons": "3.10.0",
    "react-share": "4.2.0",
    "rebass": "4.0.7",
    "slash": "^3.0.0",
    "theme-ui": "0.3.1",
    "typescript": "3.9.7"
  },
  "devDependencies": {
    "@types/node": "14.0.18",
    "@types/react": "16.9.38",
    "@types/rebass": "4.0.6",
    "@types/rebass__forms": "^4.0.2",
    "@types/theme-ui": "0.3.4",
    "husky": "4.2.5",
    "prettier": "2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "compile": "tsc",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["eslint --fix","prettier --write"]
  }
}
